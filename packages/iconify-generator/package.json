{
  "name": "@stx/iconify-generator",
  "version": "0.0.1",
  "description": "Generator for Iconify icon packages",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "type": "module",
  "bin": {
    "stx-iconify": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "bun build.ts",
    "dev": "bun build.ts --watch"
  },
  "dependencies": {
    "@stx/iconify-core": "workspace:*"
  },
  "keywords": [
    "iconify",
    "icons",
    "generator",
    "svg",
    "stx"
  ]
}
